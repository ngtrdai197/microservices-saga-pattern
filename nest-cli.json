{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/order/src",
  "monorepo": true,
  "root": "apps/order",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/order/tsconfig.app.json"
  },
  "projects": {
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "kafka": {
      "type": "library",
      "root": "libs/kafka",
      "entryFile": "index",
      "sourceRoot": "libs/kafka/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kafka/tsconfig.lib.json"
      }
    },
    "grpc": {
      "type": "library",
      "root": "libs/grpc",
      "entryFile": "index",
      "sourceRoot": "libs/grpc/src",
      "compilerOptions": {
        "tsConfigPath": "libs/grpc/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "product": {
      "type": "application",
      "root": "apps/product",
      "entryFile": "main",
      "sourceRoot": "apps/product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product/tsconfig.app.json"
      }
    },
    "biz-product": {
      "type": "library",
      "root": "libs/biz-product",
      "entryFile": "index",
      "sourceRoot": "libs/biz-product/src",
      "compilerOptions": {
        "tsConfigPath": "libs/biz-product/tsconfig.lib.json"
      }
    }
  }
}